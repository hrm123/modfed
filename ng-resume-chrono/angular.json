{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"profesnl": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/profesnl",
			"sourceRoot": "projects/profesnl/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/profesnl",
						"index": "projects/profesnl/src/index.html",
						"main": "projects/profesnl/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/profesnl/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/profesnl/src/favicon.ico",
							"projects/profesnl/src/assets"
						],
						"styles": [
							"projects/profesnl/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/profesnl/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/profesnl/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "profesnl:build:production",
							"extraWebpackConfig": "projects/profesnl/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "profesnl:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4002,
						"publicHost": "http://localhost:4002",
						"extraWebpackConfig": "projects/profesnl/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "profesnl:build",
						"extraWebpackConfig": "projects/profesnl/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/profesnl/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/profesnl/src/favicon.ico",
							"projects/profesnl/src/assets"
						],
						"styles": [
							"projects/profesnl/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
